#include<stdio.h>
int swap(int  , int );
int  main()
{
    int a,b;
    printf("enter the two number a &b :");
    scanf("%d%d",&a,&b);
    printf("before swap a=%d b=%d\n",a,b);
    swap(a,b);
    printf("after swap a=%d b=%d",b,a);
    return 0;
}
int swap(int x,int y)
{
    int t;
    t=x;
    x=y;
    y=t;
}
