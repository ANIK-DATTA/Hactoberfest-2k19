#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    for(int i=1;n>=1;i++)
    {
        n=n-i;
        if(n<=0)
        { printf("Patlu");
        break;
        }
        n=n-2*i;
        if(n<=0)
        {
            printf("Motu");
            break;
        }
    }
    return 0;
}
